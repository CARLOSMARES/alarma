if(NOT DEFINED CMAKE_INSTALL_PREFIX)
    set(CMAKE_INSTALL_PREFIX "/usr/local")
endif()
if(EXISTS "${CMAKE_INSTALL_PREFIX}/bin/alarma")
    file(REMOVE "${CMAKE_INSTALL_PREFIX}/bin/alarma")
endif()
if(EXISTS "${CMAKE_INSTALL_PREFIX}/share/applications/alarma.desktop")
    file(REMOVE "${CMAKE_INSTALL_PREFIX}/share/applications/alarma.desktop")
endif()